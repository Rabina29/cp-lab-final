#include <bits/stdc++.h>
using namespace std;

int main() {
    vector<vector<int>> g{{1,2},{0,3},{0},{1}};
    vector<int> vis(4);
    queue<int> q;

    q.push(0); vis[0] = 1;
    while(!q.empty()){
        int u = q.front(); q.pop();
        cout << u << " ";
        for(int v : g[u])
            if(!vis[v]) vis[v] = 1, q.push(v);
    }
}
